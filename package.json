{
  "name": "aeropath",
  "version": "1.0.0",
  "main": "app.js",
  "keywords": [],
  "author": "Melih Can AYDIN",
  "license": "ISC",
  "description": "AeroPath",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc && cp -R src/data dist/data",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up service",
    "docker:test": "docker-compose up test",
    "docker:down": "docker-compose down",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.5.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  }
}
